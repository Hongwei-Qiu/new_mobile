<view class="detail"><uni-nav-bar vue-id="240442ed-1" left-icon="back" title="退货单详情" color="#1A1A1A" data-event-opts="{{[['^clickLeft',[['goPage']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-nav-bar><view style="height:50px;"></view><view class="list_box"><view class="top_box"><view class="title">{{'单号：'+listDetail.order_sn+''}}</view><view class="content">{{'供应商：'+listDetail.supplier_name+''}}</view><view class="content">退货金额：<text class="red_font">{{"¥"+listDetail.net_receipts}}</text></view><view class="content">支付状态：<block wx:if="{{listDetail.pay_status==0}}"><text class="red_font">未支付</text></block><block wx:if="{{listDetail.pay_status==1}}"><text class="red_font">{{"实收:"+listDetail.net_receipts}}</text></block></view><view class="content">{{'创建时间：'+listDetail.create_time+''}}</view></view><block wx:for="{{listDetail.list}}" wx:for-item="item" wx:for-index="index"><view class="item"><view class="name">{{''+item.title+''}}</view><view class="txt"><view>规格</view><view class="right">{{item.attr_title?item.attr_title:'/'}}</view></view><view class="txt"><view>退货数量</view><view class="right">{{item.num+item.unit}}</view></view><view class="txt"><view>退货单价</view><view class="right">{{"¥"+item.price}}</view></view><view class="txt"><view>备注</view><view class="right">{{''+item.remark+''}}</view></view></view></block></view></view>